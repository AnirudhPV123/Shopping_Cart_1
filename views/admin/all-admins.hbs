<section>
    <div class="container">

        <table class="table mt-5">
            <thead>
                <tr>
                    <th scope="col">No.</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                                        <th scope="col">Remove</th>

                    

                </tr>
            </thead>
            <tbody>
                {{#each allAdmins}}
                <tr>
                    <td>1</td>
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                    <td><a href="/admin/remove-admins/{{this._id}}" class="btn btn-danger"  onclick="return confirm('Are you want to delete this admin ?')">Remove</a></td>
            
                {{/each}}


            </tbody>
        </table>
    </div>
</section>